{
	"compilerOptions": {
		"paths": {
			"$static": [
				"../static"
			],
			"$static/*": [
				"../static/*"
			],
			"$assets": [
				"../src/assets"
			],
			"$assets/*": [
				"../src/assets/*"
			],
			"$routes": [
				"../src/routes"
			],
			"$routes/*": [
				"../src/routes/*"
			],
			"$locales": [
				"../src/locales"
			],
			"$locales/*": [
				"../src/locales/*"
			],
			"$scripts": [
				"../src/scripts"
			],
			"$scripts/*": [
				"../src/scripts/*"
			],
			"$store": [
				"../src/store"
			],
			"$store/*": [
				"../src/store/*"
			],
			"$src": [
				"../src"
			],
			"$src/*": [
				"../src/*"
			],
			"$lib": [
				"../src/lib"
			],
			"$lib/*": [
				"../src/lib/*"
			]
		},
		"rootDirs": [
			"..",
			"./types"
		],
		"verbatimModuleSyntax": true,
		"isolatedModules": true,
		"lib": [
			"esnext",
			"DOM",
			"DOM.Iterable"
		],
		"moduleResolution": "bundler",
		"module": "esnext",
		"noEmit": true,
		"target": "esnext"
	},
	"include": [
		"ambient.d.ts",
		"non-ambient.d.ts",
		"./types/**/$types.d.ts",
		"../vite.config.js",
		"../vite.config.ts",
		"../src/**/*.js",
		"../src/**/*.ts",
		"../src/**/*.svelte",
		"../tests/**/*.js",
		"../tests/**/*.ts",
		"../tests/**/*.svelte"
	],
	"exclude": [
		"../node_modules/**",
		"../src/service-worker.js",
		"../src/service-worker/**/*.js",
		"../src/service-worker.ts",
		"../src/service-worker/**/*.ts",
		"../src/service-worker.d.ts",
		"../src/service-worker/**/*.d.ts"
	]
}