{
	"name": "empathy-link",
	"version": "0.0.1",
	"private": true,
	"engines": {
		"node": ">=20.10.0"
	},
	"prepare": "svelte-kit sync",
	"scripts": {
		"dev": "vite dev --port 3000 --host",
		"dev debug": "vite dev --port 3000 --host --debug",
		"build": "rimraf build && rimraf .svelte-kit && svelte-kit sync && vite build",
		"buildprod": "vite -r dotenv/config build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"sync": "npx cap sync",
		"android": "npx cap open android"
	},
	"devDependencies": {
		"@playwright/test": "^1.43.0",
		"@sveltejs/adapter-auto": "^3.2.0",
		"@sveltejs/adapter-node": "^4.0.1",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.5.27",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/uuid": "^10.0.0",
		"autoprefixer": "^10.4.19",
		"d3": "^7.9.0",
		"dotenv": "^16.4.5",
		"jimp": "^0.22.12",
		"p5": "^1.11.3",
		"pocketbase": "^0.21.1",
		"postcss": "^8.4.38",
		"postcss-import": "^16.1.0",
		"postcss-load-config": "^5.0.3",
		"prettier": "^3.2.5",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.5.13",
		"rimraf": "^6.0.1",
		"sass": "^1.74.1",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-copy": "^2.0.0",
		"svelte-preprocess": "^6.0.0",
		"svelte-sonner": "^0.3.21",
		"sveltekit-i18n": "^2.4.2",
		"sveltekit-superforms": "^2.12.2",
		"tailwindcss": "^3.4.3",
		"tslib": "^2.6.2",
		"typescript": "^5.5.0",
		"vite": "^5.4.4",
		"zod": "^3.23.8"
	},
	"type": "module",
	"dependencies": {
		"@google/genai": "^0.8.0",
		"@internationalized/date": "^3.5.2",
		"@internationalized/number": "^3.5.1",
		"bits-ui": "^1.4.6",
		"clsx": "^2.1.0",
		"formsnap": "^1.0.0",
		"lucide-svelte": "^0.487.0",
		"marked": "^15.0.7",
		"mode-watcher": "^1.0.6",
		"posthog-js": "^1.166.1",
		"resize-observer-polyfill": "^1.5.1",
		"tailwind-merge": "^2.2.2",
		"tailwind-variants": "^0.2.1",
		"uuid": "^10.0.0",
		"vaul-svelte": "^0.3.0"
	}
}
